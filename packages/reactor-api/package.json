    "@graphql-codegen/typescript-generic-sdk": "catalog:",
    "@graphql-codegen/typescript-operations": "catalog:",
    "@graphql-codegen/typescript-resolvers": "catalog:",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "@types/ws": "^8.18.1",
