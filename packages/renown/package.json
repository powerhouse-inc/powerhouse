{
  "name": "@renown/sdk",
  "version": "0.15.5-staging.0",
  "description": "",
  "license": "AGPL-3.0-only",
  "private": false,
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.browser.ts",
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    "#*": "./dist/src/*",
    "#init/env": {
      "node": "./dist/src/init.node.js",
      "default": "./dist/src/init.browser.js"
    }
  },
  "files": [
    "dist"
  ],
  "nx-release-publish": {
    "options": {
      "packageRoot": "dist/{projectRoot}"
    }
  },
  "scripts": {
    "build:tsc": "tsc -p tsconfig.node.json --noEmit && tsc -p tsconfig.browser.json --noEmit",
    "build:tsc:node": "tsc -p tsconfig.node.json --noEmit",
    "build:tsc:browser": "tsc -p tsconfig.browser.json --noEmit",
    "build": "tsc --build",
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "clean:node_modules": "rimraf node_modules"
  },
  "peerDependencies": {
    "document-model": "workspace:*"
  },
  "devDependencies": {
    "document-model": "workspace:*"
  },
  "dependencies": {
    "@didtools/key-did": "^1.0.0",
    "did-jwt": "^8.0.14",
    "did-jwt-vc": "^4.0.12",
    "did-resolver": "^4.1.0",
    "key-did-resolver": "^4.0.0"
  }
}
