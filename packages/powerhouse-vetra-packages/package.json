{
  "name": "@powerhousedao/powerhouse-vetra-packages",
  "version": "6.0.0-dev.56",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-only",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/powerhouse"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./editors/*": {
      "types": "./dist/editors/*/index.d.ts",
      "default": "./dist/editors/*/index.js"
    },
    "./editors": {
      "import": "./dist/editors/index.js",
      "types": "./dist/editors/index.d.ts"
    },
    "./editors/style.css": "./dist/editors/style.css"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "build:css": "pnpm exec tailwindcss -i ./editors/style.css -o ./dist/editors/style.css",
    "test:watch": "vitest",
    "generate": "ph-cli generate",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "migrate": "ph-cli migrate",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh"
  },
  "dependencies": {
