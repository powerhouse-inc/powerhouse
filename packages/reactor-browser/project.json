{
  "name": "@powerhousedao/reactor-browser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/reactor-browser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm check-types",
          "NODE_ENV=production vite build"
        ],
        "cwd": "packages/reactor-browser",
        "parallel": false
      },
      "dependsOn": ["^build"]
    },
    "check-types": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/reactor-browser"
      }
    }
  },
  "tags": []
} 