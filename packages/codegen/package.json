{
  "name": "@powerhousedao/codegen",
  "version": "5.1.0-dev.15",
  "license": "AGPL-3.0-only",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/powerhouse"
  },
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "copy-hygen": "rm -rf dist/src/codegen/.hygen && cp -r src/codegen/.hygen dist/src/codegen/",
    "build": "npm run copy-hygen",
    "build:create-lib": "tsc --project src/create-lib/tsconfig.json",
    "publish:create-lib": "npm publish --cwd ./src/create-lib/",
    "start": "tsx src/cli.ts",
    "test": "npm run copy-hygen && vitest run",
    "test:ts-morph": "USE_TS_MORPH=true npm run test",
    "test:keep": "KEEP_TEST_OUTPUT=true npm run test",
    "test:keep:ts-morph": "USE_TS_MORPH=true KEEP_TEST_OUTPUT=true npm run test"
  },
  "dependencies": {
    "@acaldas/graphql-codegen-typescript-validation-schema": "^0.12.4",
    "@anatine/zod-mock": "^3.13.3",
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@powerhousedao/common": "workspace:*",
    "@powerhousedao/config": "workspace:*",
    "@powerhousedao/design-system": "workspace:*",
    "@powerhousedao/document-engineering": "^1.30.0",
    "@powerhousedao/reactor-browser": "workspace:*",
    "@tmpl/core": "npm:@jsr/tmpl__core@^0.6.3",
    "@types/node": "^24.6.1",
    "arg": "^5.0.2",
    "change-case": "^4.1.2",
    "document-model": "workspace:*",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "graphql": "^16.11.0",
    "hygen": "^6.2.11",
    "kysely": "^0.28.2",
    "kysely-pglite": "^0.6.1",
    "prettier": "^3.6.2",
    "read-pkg": "^9.0.1",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/core": "^4.0.2",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "copyfiles": "^2.4.1",
    "document-drive": "workspace:*",
    "esbuild-fix-imports-plugin": "^1.0.7",
    "husky": "^8.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vitest": "^3.2.4",
    "zod": "^3.24.3"
  }
}
