  "license": "AGPL-3.0-only",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/powerhouse"
  },
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "copy-hygen": "cp -r src/codegen/.hygen dist/src/codegen/",
    "build": "npm run copy-hygen",
    "build:create-lib": "tsc --project src/create-lib/tsconfig.json",
    "publish:create-lib": "npm publish --cwd ./src/create-lib/",
    "start": "tsx src/cli.ts",
    "pretest": "rimraf ./src/codegen/__tests__/.out && rimraf ./src/codegen/__tests__/.test-project",
    "test": "npm run copy-hygen && vitest run"
  },
  "dependencies": {
    "@acaldas/graphql-codegen-typescript-validation-schema": "^0.12.3",
