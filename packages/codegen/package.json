    "@graphql-codegen/core": "^4.0.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "copyfiles": "^2.4.1",
    "document-drive": "workspace:*",
    "esbuild-fix-imports-plugin": "^1.0.7",
    "graphql": "^16.11.0",
    "husky": "^8.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vitest": "^3.2.4",
    "zod": "^3.24.3"
  }
