{
  "name": "@powerhousedao/scalars",
  "version": "1.13.0",
  "license": "AGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/src/index.d.ts",
  "main": "dist/cjs/src/index.js",
  "module": "dist/es/src/index.js",
  "scripts": {
    "check-types": "tsc",
    "lint": "eslint .",
    "lint:nx": "eslint . --fix --quiet",
    "lint:fix": "eslint --fix",
    "postlint": "npm run check-types",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test": "vitest run",
    "clean": "rimraf dist",
    "clean:node_modules": "rimraf node_modules"
  },
  "devDependencies": {
    "glob": "^11.0.0",
    "graphql": "^16.9.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "graphql": "^16.9.0",
    "zod": "^3.23.8"
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/es/src/index.js"
    },
    "./src\\scalars\\index": {
      "types": "./dist/types/src/scalars/src\\scalars\\index.d.ts",
      "require": "./dist/cjs/src/scalars/src\\scalars\\index.js",
      "import": "./dist/es/src/scalars/src\\scalars\\index.js"
    },
    "./src\\scalars\\EthereumAddress": {
      "types": "./dist/types/src/scalars/src\\scalars\\EthereumAddress.d.ts",
      "require": "./dist/cjs/src/scalars/src\\scalars\\EthereumAddress.js",
      "import": "./dist/es/src/scalars/src\\scalars\\EthereumAddress.js"
    },
    "./src\\scalars\\EmailAddress": {
      "types": "./dist/types/src/scalars/src\\scalars\\EmailAddress.d.ts",
      "require": "./dist/cjs/src/scalars/src\\scalars\\EmailAddress.js",
      "import": "./dist/es/src/scalars/src\\scalars\\EmailAddress.js"
    },
    "./src\\scalars\\AmountTokens": {
      "types": "./dist/types/src/scalars/src\\scalars\\AmountTokens.d.ts",
      "require": "./dist/cjs/src/scalars/src\\scalars\\AmountTokens.js",
      "import": "./dist/es/src/scalars/src\\scalars\\AmountTokens.js"
    },
    "./src\\scalars\\AmountPercentage": {
      "types": "./dist/types/src/scalars/src\\scalars\\AmountPercentage.d.ts",
      "require": "./dist/cjs/src/scalars/src\\scalars\\AmountPercentage.js",
      "import": "./dist/es/src/scalars/src\\scalars\\AmountPercentage.js"
    }
  }
