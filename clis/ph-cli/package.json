{
  "name": "@powerhousedao/ph-cli",
  "version": "6.0.0-dev.13",
  "description": "",
  "license": "AGPL-3.0-only",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/powerhouse"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ph-cli": "dist/src/cli.js",
    "ph-cli-legacy": "dist/src/cli.old.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "generate-commands-md": "tsx scripts/generate-commands-md.ts",
    "generate-version": "tsx scripts/generate-version.ts",
    "build": "npm run generate-commands-md && npm run generate-version && tsc && npm run copy-scripts",
    "copy-scripts": "copyfiles scripts/* dist/",
    "dev": "concurrently -P 'pnpm -w run tsc --watch' 'nodemon --watch \"../..\" -e ts,tsx,js,json dist/src/cli.js -- {@}' --",
    "cli": "node ./dist/src/cli.js",
    "test": "vitest --run"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@types/pg": "^8.11.10",
    "@types/npm-package-arg": "^6.1.4",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "document-drive": "workspace:*",
    "document-model": "workspace:*",
    "nodemon": "^3.1.9",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "workspace:*",
    "@powerhousedao/codegen": "workspace:*",
    "@powerhousedao/common": "workspace:*",
    "@powerhousedao/config": "workspace:*",
    "@powerhousedao/reactor": "workspace:*",
    "@powerhousedao/switchboard": "workspace:*",
    "@renown/sdk": "workspace:*",
    "@tmpl/core": "npm:@jsr/tmpl__core@^0.6.3",
    "chalk": "^5.6.2",
    "cmd-ts": "^0.14.3",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "kysely": "0.28.8",
    "pg": "^8.13.0",
    "npm-package-arg": "^13.0.2",
    "package-manager-detector": "^1.6.0",
    "read-pkg": "^9.0.1",
    "ts-morph": "^26.0.0",
    "vite": "^7.2.6",
    "write-package": "^7.2.0"
  }
}
