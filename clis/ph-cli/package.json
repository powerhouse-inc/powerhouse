{
  "name": "@powerhousedao/ph-cli",
  "version": "4.1.0-dev.33",
  "description": "",
  "license": "AGPL-3.0-only",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/powerhouse-inc/powerhouse"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ph-cli": "dist/src/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "generate-commands-md": "tsx scripts/generate-commands-md.ts",
    "generate-version": "tsx scripts/generate-version.ts",
    "build": "npm run generate-commands-md && npm run generate-version && npm run copy-scripts",
    "copy-scripts": "copyfiles scripts/* dist/",
    "dev": "concurrently -P 'pnpm -w run tsc --watch' 'nodemon --watch \"../..\" -e ts,tsx,js,json dist/src/cli.js -- {@}' --",
    "test": "vitest --run"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "workspace:*",
    "@powerhousedao/codegen": "workspace:*",
    "@powerhousedao/config": "workspace:*",
    "@powerhousedao/connect": "workspace:*",
    "@powerhousedao/reactor-local": "workspace:*",
    "@powerhousedao/switchboard": "workspace:*",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "document-drive": "workspace:*",
    "document-model": "workspace:*",
    "pm2": "^5.4.3"
  }
}
