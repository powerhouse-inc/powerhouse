{
  "name": "connect-e2e",
  "version": "1.0.237-dev.0",
  "description": "",
  "type": "module",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "development": "./index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "development": "./document-models/index.ts",
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "development": "./editors/index.ts",
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./document-models/*": {
      "development": "./document-models/*/index.ts",
      "types": "./dist/document-models/*/index.d.ts",
      "default": "./dist/document-models/*/index.js"
    },
    "./editors/*": {
      "development": "./editors/*/index.ts",
      "types": "./dist/editors/*/index.d.ts",
      "default": "./dist/editors/*/index.js"
    },
    "./subgraphs": {
      "development": "./subgraphs/index.ts",
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "development": "./processors/index.ts",
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "development": "./powerhouse.manifest.json",
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "generate": "ph-cli generate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@powerhousedao/builder-tools": "workspace:*",
    "@powerhousedao/e2e-utils": "workspace:*",
    "@powerhousedao/ph-cli": "workspace:*",
    "@powerhousedao/connect": "workspace:*",
    "@powerhousedao/reactor-browser": "workspace:*",
    "@types/node": "catalog:",
    "change-case": "catalog:",
    "document-model": "workspace:*",
    "jszip": "^3.10.1",
    "monocart-coverage-reports": "^2.12.8",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vite": "catalog:",
    "vite-plugin-node-polyfills": "catalog:",
    "@electric-sql/pglite": "catalog:"
  },
  "dependencies": {
    "@powerhousedao/design-system": "workspace:*"
  }
}
