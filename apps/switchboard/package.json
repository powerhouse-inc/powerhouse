    "@powerhousedao/reactor-api": "workspace:*",
    "@powerhousedao/scalars": "workspace:*",
    "@sentry/node": "^9.6.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "document-drive": "workspace:*",
    "document-model": "workspace:*",
    "dotenv": "^16.4.7",
    "exponential-backoff": "^3.1.1",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
