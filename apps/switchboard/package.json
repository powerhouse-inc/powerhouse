  "main": "dist/src/index.js",
  "bin": {
    "switchboard": "dist/src/index.js"
  },
  "scripts": {
    "build:tsc": "tsc --build",
    "build": "pnpm run build:tsc",
    "start": "node dist/src/index.js",
